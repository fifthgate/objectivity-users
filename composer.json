{
    "name": "fifthgate/objectivity-users",
    "description": "A Domain-Driven Users package for Laravel 8",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Baynham",
            "email": "sam@fifthgate.net"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Fifthgate\\Objectivity\\Users\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fifthgate\\Objectivity\\Users\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.2",
        "fifthgate/objectivity-repos": "~v2.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.21",
        "orchestra/testbench-browser-kit": "^8.5",
        "phpunit/phpunit": "^9.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fifthgate/objectivity-core"
        },
        {
            "type": "vcs",
            "url": "https://github.com/fifthgate/objectivity-repos"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Fifthgate\\Objectivity\\Users\\UserServiceProvider"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        }
    }
}
